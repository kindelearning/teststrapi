{
  "kind": "collectionType",
  "collectionName": "payment_methods",
  "info": {
    "singularName": "payment-method",
    "pluralName": "payment-methods",
    "displayName": "Payment Method",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "default": "Name"
    },
    "Number": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "default": "123424645"
    },
    "ExpiryDate": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date",
      "default": "2025-01-14"
    },
    "CVV": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "default": 123,
      "max": 999,
      "min": 100
    },
    "myUsers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "myPaymentMethods"
    }
  }
}
